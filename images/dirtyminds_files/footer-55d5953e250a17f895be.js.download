        .ft-right-et {\n            .fl();\n            margin-top: 16px;\n            margin-bottom: 20px;\n            > li {\n                .fl();\n                width: 36px;\n                height: 36px;\n                .margin-r(10px);\n                line-height: 32px;\n                > a {\n                    font-size: 36px;\n                    color: #222;\n                    opacity: 1;\n                    transition: all .3s ease-in-out 0s;\n                    cursor: pointer;\n                    &:focus{\n                        text-decoration: none;\n                    }\n                    &:hover {\n                        text-decoration: none;\n                    }\n                }\n            }\n            &:last-child {\n                > li > a {\n                    font-size: 30px;\n                }\n            }\n        }\n        .ft-right-input {\n            display: flex;\n            .c-privacy-check{\n                margin-top: 8px;\n                padding-top: 0;\n                border-top: none;\n            }\n            .c-input{\n                .input-error-txt{\n                    >a{\n                        color: #d53333;\n                        text-decoration: underline;\n                    }\n                }\n                input::placeholder {\n                  color: #bbb;\n                }\n            }\n            & when(@IS_RW) {\n                .c-input{\n                    input {\n                        border: none;\n                        border-radius: 18px 0 0 18px;\n                    }\n                }\n                .ft-right-btn {\n                    .she-btn-black {\n                        border-radius: 0 18px 18px 0;\n                    }\n                }\n            }\n        }\n        .ft-right-email {\n            > h6 {\n                color: #222;\n                font-weight: bold;\n                line-height: 14px;\n                .txt-l();\n                margin-bottom: 10px;\n            }\n        }\n        .ft-right-payment {\n            > h6 {\n                color: #222;\n                font-weight: bold;\n                line-height: 14px;\n                .txt-l();\n                margin: 30px 0 15px 0;\n            }\n            .bank-img {\n                .c-img-con-m {\n                    display: inline-block;\n                    height: 24px;\n                    .margin-r(10px);\n                }\n            }\n        }\n    }\n    .ft-bottom {\n        font-size: 12px;\n        color: #666;\n        margin-top: 20px;\n        .padding-l(0);\n        .ft-bottom-lang {\n            .padding-l(10px);\n        }\n        .ft-bottom-add {\n            margin-top: 5px;\n            max-width: 520px;\n            .padding-l(0);\n            > li {\n                display: inline-block;\n                padding: 0 10px;\n                margin-bottom: 10px;\n                .border-r(1px, #ccc);\n                > a {\n                    color: #666;\n                    text-decoration: underline;\n                }\n            }\n            > li:last-child {\n                border: 0;\n            }\n            .footer-location-ctn {\n                .footer-location-country {\n                    color:rgba(0,0,0,0.8);\n                    cursor: pointer;\n                    a {\n                        color:rgba(0,0,0,0.8);\n                        text-decoration: underline;\n                    }\n                }\n            }\n        }\n    }\n    .ft-bottom-icon {\n        margin-top: 20px;\n        .txt-l();\n        > a {\n            display: inline-block;\n            img {\n                display: inline-block;\n                height: 24px;\n            }\n            .dmca-img {\n                width: 72px;\n            }\n            .margin-r(15px);\n            &:last-child {\n                .margin-r(0);\n            }\n        }\n    }\n\n}\n\n.ft